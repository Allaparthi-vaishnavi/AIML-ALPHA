    "def binarize(lst, threshold):\n",
    "    return [1 if x >= threshold else 0 for x in lst]\n",
    "input_list = [1.5, 2.3, 0.8, 3.0]\n",
    "threshold = 2.0\n",
    "binarized_result = binarize(input_list, threshold)\n",
    "print(binarized_result)"
